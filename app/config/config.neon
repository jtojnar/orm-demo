extensions:
	orm: Nextras\Orm\Bridges\NetteDI\OrmExtension


parameters:


php:
	date.timezone: Europe/Prague


nette:
	application:
		errorPresenter: Error
		mapping:
			*: OrmDemo\*Module\*Presenter

	session:
		expiration: 14 days


orm:
	model: OrmDemo\Orm


services:
	routerFactory: OrmDemo\RouterFactory()
	router: @routerFactory::createRouter
